package one.innovation.digital.arrays;

public class ArraysApp {

	public static void main(String[] args) {
		
		int[] arrays = new int [5];
		arrays[0] = 1;
		arrays[0] = 5;
		arrays[0] = 3;
		
		for (int i : arrays) {
			System.out.print(i + " ");
		}
		
		arrays[3] = 10;
		System.out.println();
		for (int i : arrays) {
			System.out.print(i + " ");
		}
		
		
		
		

	}

}
