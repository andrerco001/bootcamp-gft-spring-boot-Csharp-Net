package one.innovation.digital.arrays;

public class ArraysApp {

	public static void main(String[] args) {
		
		int[] arrays = {1, 5, 3};
		for (int i : arrays) {
			System.out.print(i + " ");
		}
		
		arrays[3] = 78;
		System.out.println();
		for (int i : arrays) {
			System.out.print(i + " ");
		}
		
		
		
		

	}

}
